Cell[CellGroupData[{

Cell[TextData[{
 "Suppose, for a contradiction, that ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["y", "TI"], "1"], ",", 
         SubscriptBox[
          StyleBox["y", "TI"], "2"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "$y_ 1, y_ 2 $", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "942a4a15-7c66-4fb9-ad16-61d880317d42"],
 "are linearly dependent on ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["I", "TI"], TraditionalForm], "errors" -> {}, "input" -> 
     "$I$", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "040a68d4-e729-4775-aa85-4c91a6bcf565"],
 ". Then there exist constants ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["c", "TI"], "1"], ",", 
         SubscriptBox[
          StyleBox["c", "TI"], "2"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "$c_ 1, c_ 2 $", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "79802648-ef33-42cf-be48-69bc7ff87da1"],
 ", not both zero, such that\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["c", "TI"], "1"], 
         SubscriptBox[
          StyleBox["y", "TI"], "1"], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], "+", 
         SubscriptBox[
          StyleBox["c", "TI"], "2"], 
         SubscriptBox[
          StyleBox["y", "TI"], "2"], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], "\[LongEqual]", "0    ", 
         RowBox[{"for all ", 
           Cell[
            StyleBox["x", "TI"], "InlineFormula"], " in ", 
           Cell[
            StyleBox["I", "TI"], "InlineFormula"]}], "."}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "$c_ 1 y_ 1 (x) + c_ 2 y_ 2 (x) = 0\\qquad\\text {for all \\(x\\) in \
\\(I\\)} .\n   $", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "3d3e0aa0-57e3-4d08-969a-3a20d46328de"],
 "\nDifferentiating gives\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["c", "TI"], "1"], 
         SubsuperscriptBox[
          StyleBox["y", "TI"], "1", "\[Prime]"], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], "+", 
         SubscriptBox[
          StyleBox["c", "TI"], "2"], 
         SubsuperscriptBox[
          StyleBox["y", "TI"], "2", "\[Prime]"], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], "\[LongEqual]", "0    ", 
         RowBox[{"for all ", 
           Cell[
            StyleBox["x", "TI"], "InlineFormula"], " in ", 
           Cell[
            StyleBox["I", "TI"], "InlineFormula"]}], "."}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "$c_ 1 y_ 1' (x) + c_ 2 y_ 2' (x) = 0\\qquad\\text {for all \\(x\\) in \
\\(I\\)} .\n    $", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "084e01c6-5188-41ab-b916-9a7dc078b59d"],
 "\nEvaluating these two identities at ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["x", "TI"], "\[LongEqual]", 
         SubscriptBox[
          StyleBox["x", "TI"], "0"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "$x = x_ 0 $", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "ab6c0234-fc10-48d5-8d28-46ff7f0e6440"],
 " yields\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         RowBox[{"[", 
           Cell[
            BoxData[
             FormBox[
              GridBox[{{
                 Cell[
                  TextData[{" ", 
                    Cell[
                    RowBox[{
                    SubscriptBox[
                    StyleBox["y", "TI"], "1"], "(", 
                    SubscriptBox[
                    StyleBox["x", "TI"], "0"], ")"}], "InlineFormula"], 
                    " "}]], 
                 Cell[
                  TextData[{" ", 
                    Cell[
                    RowBox[{
                    SubscriptBox[
                    StyleBox["y", "TI"], "2"], "(", 
                    SubscriptBox[
                    StyleBox["x", "TI"], "0"], ")"}], "InlineFormula"], 
                    " "}]]}, {
                 Cell[
                  TextData[{" ", 
                    Cell[
                    RowBox[{
                    SubsuperscriptBox[
                    StyleBox["y", "TI"], "1", "\[Prime]"], "(", 
                    SubscriptBox[
                    StyleBox["x", "TI"], "0"], ")"}], "InlineFormula"], 
                    " "}]], 
                 Cell[
                  TextData[{" ", 
                    Cell[
                    RowBox[{
                    SubsuperscriptBox[
                    StyleBox["y", "TI"], "2", "\[Prime]"], "(", 
                    SubscriptBox[
                    StyleBox["x", "TI"], "0"], ")"}], "InlineFormula"], 
                    " "}]]}}], TraditionalForm]]], "]"}], 
         RowBox[{"[", 
           Cell[
            BoxData[
             FormBox[
              GridBox[{{
                 Cell[
                  TextData[{" ", 
                    Cell[
                    SubscriptBox[
                    StyleBox["c", "TI"], "1"], "InlineFormula"], " "}]]}, {
                 Cell[
                  TextData[{" ", 
                    Cell[
                    SubscriptBox[
                    StyleBox["c", "TI"], "2"], "InlineFormula"], " "}]]}}], 
              TraditionalForm]]], "]"}], "\[LongEqual]", 
         RowBox[{"[", 
           Cell[
            BoxData[
             FormBox[
              GridBox[{{
                 Cell[
                  TextData[{" ", 
                    Cell["0", "InlineFormula"], " "}]]}, {
                 Cell[
                  TextData[{" ", 
                    Cell["0", "InlineFormula"], " "}]]}}], TraditionalForm]]],
            "]"}], "."}], TraditionalForm], "errors" -> {}, "input" -> 
     "$\n \\begin {bmatrix} y_ 1 (x_ 0) & y_ 2 (x_ 0) \\\\[4 pt] y_ 1' (x_ 0) \
& y_ 2' (x_ 0)\\end {bmatrix}\\begin {bmatrix} c_ 1 \\\\ c_ 2\\end {bmatrix} \
= \\begin {bmatrix} 0 \\\\ 0\\end {bmatrix} .\n    $", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "cfd99a18-c093-4da1-a0c5-0dd18cd917d3"]
}], "Subtitle",
